<div class="row" style="width : 100%; height: 100vh">
    {{#row}}
        <div class="{{divWidth}}">
            <div class="draggable"></div>
            {{#widgets}}
                <div class="draggable card">
                    <div id="{{widgetId}}"></div>
                    <div class="card-block">
                        <h3>{{title}}</h3>
                    </div>
                </div>
            {{/widgets}}
        </div>
    {{/row}}
</div>