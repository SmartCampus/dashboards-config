var drawLineChart = function() {
    /**
    * Create the chart when all data is loaded
    * @returns {undefined}
    */
    $({{& name}}).highcharts('StockChart', {

        yAxis: {

        title: {
        text: {{& textY}}
        },

        minTickInterval: 1,

        plotLines: [{
        value: 0,
        color: 'red',
        dashStyle: 'shortdash', //pointillé
        width: 2,
        label: { text: '0°C'}
        }]
        },

        tooltip: {
        pointFormat: {{& html}},
        valueDecimals: 2
        },

        series: temperaturesArray
    });
};
